<% if @shipments.size > 0 %>
	<table class="prices_table">
		<tr>
			<th>Id</th>
			<th>Create Date</th>
			<th>Tracking Number</th>
			<th>Customer</th>
			<th>From</th>
			<th>To</th>
			<th>Charge Customer</th>
			<th>Label</th>
			<% if current_user.admin? %>
			<th>Delete?</th>
			<% end %>
		</tr>
	<% @shipments.each do |shipment| %>
		<tr>
			<td><a href="/admin/shipment/<%= shipment.id %>"><%= shipment.id %></a></td>
			<td><%= shipment.created_at.strftime('%m/%d/%Y %l:%M %p') %></td>
			<td><%= shipment.tracking_number %></td>
			<td><a href="/admin/user/<%= shipment.user.id %>"><%= shipment.user.first_name %> <%= shipment.user.last_name %></a></td>
			<td><%= shipment.from_address.user == shipment.user ? "User" : "TVC" %></td>
			<td><%= shipment.to_address.user == shipment.user ? "User" : "TVC" %></td>
			<td>TBD</td>
			<td><a href="/shipment/<%=shipment.id%>/get_label">Label</a></td>
			<% if current_user.admin? %>
			<td><a href="<%= delete_pre_url %><%= shipment.id %>/destroy" data-confirm="This will delete the shipment and cancel it with FedEx. Are you SURE?" data-method="delete" rel="nofollow">Delete</a></td>
			<% end %>
		</tr>
	<% end %>
	</table>
<% else %>
No shipments
<% end %>